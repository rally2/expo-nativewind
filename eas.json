{
  "cli": {
    "version": ">= 0.52.0",
    "requireCommit": true
  },
  "build": {
    "development": {
      "distribution": "internal",
      "developmentClient": true,
      "channel": "dev.expo49.v1.0",
      "env": {
        "APP_ENV": "development",
        "EXPO_PUBLIC_APP_DISPLAY_NAME": "(Local) RouterWind",
        "EXPO_PUBLIC_PACKAGE": "com.rally2.routerwind.local",
        // TODO: Update this with a staging version of the icon
        "EXPO_PUBLIC_FOREGROUND_IMAGE": "./assets/dev/adaptive-icon.local.png",
        "EXPO_PUBLIC_SCHEME": "routerwindlocal",
        "EXPO_PUBLIC_BUNDLE_ID": "com.rally2.routerwind.local",
        "EXPO_PUBLIC_ICON_PATH": "./assets/dev/icon.local.png",
        "EXPO_PUBLIC_APP_VERSION": "1.0",
        "EXPO_PUBLIC_BUILD_NUMBER": "1",
        "EXPO_PUBLIC_OTA": "0"
      }
    },
    "staging": {
      "distribution": "store",
      "channel": "staging.expo49.v1.0",
      "env": {
        "APP_ENV": "staging",
        "EXPO_PUBLIC_APP_DISPLAY_NAME": "(Staging) RouterWind",
        "EXPO_PUBLIC_PACKAGE": "com.rally2.routerwind.staging",
        // TODO: Update this with a staging version of the icon
        "EXPO_PUBLIC_FOREGROUND_IMAGE": "./assets/staging/adaptive-icon.staging.png",
        "EXPO_PUBLIC_SCHEME": "routerwindstaging",
        "EXPO_PUBLIC_BUNDLE_ID": "com.rally2.routerwind.staging",
        "EXPO_PUBLIC_ICON_PATH": "./assets/staging/icon.staging.png",
        "EXPO_PUBLIC_APP_VERSION": "1.0",
        "EXPO_PUBLIC_BUILD_NUMBER": "1",
        "EXPO_PUBLIC_OTA": "0"
      }
    },
    "production": {
      "distribution": "store",
      "channel": "prod.expo49.v1.0",
      "env": {
        "APP_ENV": "production",
        "EXPO_PUBLIC_APP_DISPLAY_NAME": "RouterWind",
        "EXPO_PUBLIC_PACKAGE": "com.rally2.routerwind",
        // TODO: Update this with a staging version of the icon
        "EXPO_PUBLIC_FOREGROUND_IMAGE": "./assets/adaptive-icon.png",
        "EXPO_PUBLIC_SCHEME": "routerwind",
        "EXPO_PUBLIC_BUNDLE_ID": "com.rally2.routerwind",
        "EXPO_PUBLIC_ICON_PATH": "./assets/icon.png",
        "EXPO_PUBLIC_APP_VERSION": "1.0",
        "EXPO_PUBLIC_BUILD_NUMBER": "1",
        "EXPO_PUBLIC_OTA": "0"
      }
    }
  },
  "submit": {}
}
